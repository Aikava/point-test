{
  "name": "point-test",
  "version": "1.0.0",
  "description": "Test task for tochka bank",
  "main": "main.ts",
  "scripts": {
    "start:server:dev": "cross-env NODE_ENV=local NODE_PATH=./src NODE_CONFIG_DIR=./src/server/config node -r ts-node/register src/server/main.ts",
    "start:client:dev": "cd src/client && webpack-dev-server --mode development",
    "dev": "run-p start:*:dev",
    "start:server": "node server/main.js",
    "build:server": "tsc --project src/server/tsconfig.json",
    "build:client": "cd src/client && webpack",
    "build": "run-s build:*",
    "deploy:build": "docker build -t localhost:5000/point-test:latest ./",
    "deploy:push": "docker push localhost:5000/point-test:latest",
    "deploy": "run-s deploy:*"
  },
  "author": "Rykov Dmitry",
  "license": "ISC",
  "dependencies": {
    "@types/handlebars": "^4.1.0",
    "@types/moment": "^2.13.0",
    "css-loader": "^2.1.1",
    "express": "^4.16.4",
    "handlebars": "^4.1.2",
    "jquery": "^3.4.0",
    "moment": "^2.24.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "typescript": "^3.4.3"
  },
  "devDependencies": {
    "@types/config": "0.0.34",
    "@types/express": "4.16.1",
    "@types/jquery": "^3.3.29",
    "@types/node": "^11.13.2",
    "@types/sequelize": "^4.27.24",
    "babel-core": "^6.26.3",
    "body-parser": "^1.18.3",
    "config": "^3.1.0",
    "cross-env": "^5.2.0",
    "css-hot-loader": "^1.4.4",
    "handlebars-loader": "^1.7.1",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "pg": "^7.9.0",
    "redux": "^4.0.1",
    "sequelize": "^4.41.2",
    "sequelize-typescript": "^0.6.6",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.3",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-middleware": "^3.6.2",
    "webpack-dev-server": "^3.3.1",
    "webpack-hot-middleware": "^2.24.3"
  }
}
